     pipeline {
        agent {
        label 'slave-devopsgol'
        }
        stages {

        stage("Git Checkout"){
            steps{
                checkout scm
            }
        }

          stage("build & sonarqube") {
            agent any
            steps {
              withSonarQubeEnv('sonarqube-devops') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
